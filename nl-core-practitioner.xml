                <code value="HumanName"/>
                <profile value="http://fhir.nl/fhir/StructureDefinition/nl-core-humanname"/>
            </type>
            <mapping>
                <identity value="zib-zorgaanbieder"/>
                <map value="NL-CM:17.1.3"/>
