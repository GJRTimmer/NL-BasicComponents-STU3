                <code value="Address"/>
                <profile value="http://fhir.nl/fhir/StructureDefinition/nl-core-address"/>
            </type>
            <mapping>
                <identity value="zib-zorgaanbieder"/>
                <map value="NL-CM:17.1.7"/>
