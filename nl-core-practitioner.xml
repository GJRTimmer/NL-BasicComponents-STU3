    <copyright value="CC0"/>
    <fhirVersion value="3.0.0"/>
    <mapping>
        <identity value="zib-zorgverlener"/>
        <uri value="https://zibs.nl/wiki/Zorgverlener(NL)"/>
        <name value="Zorginformatiebouwsteen Zorgverlener"/>
