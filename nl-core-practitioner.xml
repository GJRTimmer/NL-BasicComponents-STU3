                <map value="NL-CM:0.1.18"/>
            </mapping>
        </element>
        <element id="Practitioner.telecom.system.2">
            <path value="Practitioner.telecom.system"/>
            <type>
                <code value="code"/>
            </type>
            <fixedCode value="email"/>
