                <valueSetReference>
                    <reference value="http://decor.nictiz.nl/fhir/ValueSet/2.16.840.1.113883.2.4.3.11.60.40.2.17.1.6--20150401000000"/>
                    <display value="SpecialismeUZICodelijst"/>
